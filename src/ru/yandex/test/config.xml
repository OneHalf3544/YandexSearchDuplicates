<beans xmlns="http://www.springframework.org/schema/beans"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
    
    <bean id="SearchDoubles" class="ru.yandex.test.SearchDoubles">
        <property name="dialog" ref="dialog"/>
        <property name="reportCreator" ref="reportCreator"/>
    </bean>
    
    <bean id="moikrug_ru" class="ru.yandex.test.impl.SiteParser">
        <property name="siteName" value="moikrug.ru" />
        <property name="webHarvestConfig" value="/Users/OneHalf/Documents/NetBeansProjects/Yandex/SearchDoules/src/ru/yandex/test/siteparseconfigs/moikrug.xml" />
    </bean>
    <bean id="job_ru" class="ru.yandex.test.impl.SiteParser">
        <property name="siteName" value="job.ru" />
        <property name="webHarvestConfig" value="/Users/OneHalf/Documents/NetBeansProjects/Yandex/SearchDoules/src/ru/yandex/test/siteparseconfigs/job.xml" />
    </bean>
    <bean id="habrahabr_ru" class="ru.yandex.test.impl.SiteParser">
        <property name="siteName" value="habrahabr.ru" />
        <property name="webHarvestConfig" value="/Users/OneHalf/Documents/NetBeansProjects/Yandex/SearchDoules/src/ru/yandex/test/siteparseconfigs/habrahabr.xml" />
    </bean>
    
    <bean id="dialog" class="ru.yandex.test.impl.SearchDialogImpl">
        <property name="siteParsers">
            <set>
                <ref bean="job_ru" />
                <ref bean="moikrug_ru" />
                <ref bean="habrahabr_ru" />
            </set>
        </property>
        <property name="visible" value="true"/>
    </bean>
	
    <bean id="reportCreator" class="ru.yandex.test.impl.ReportCreatorImpl">
    </bean>    
</beans>